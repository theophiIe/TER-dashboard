<!DOCTYPE html>
<html lang="fr">
<%- include('../templates/head'); -%>
<body class="d-flex flex-column min-vh-100">
<%- include('../templates/header'); -%>
<div class="container col-lg-8 px-4 py-2">
    <h1 class="display-5 fw-bold">Personnalite</h1>
    <table id="example" class="table table-striped cell-border" style="width:100%">
        <thead>
        <tr>
            <th>Nom</th>
        </tr>
        </thead>
        <tbody>
        <% for (personnalite of personnalites) { %>
            <tr>
                <td><%= personnalite.nom %></td>
            </tr>
        <% } %>
        </tbody>
    </table>
</div>
<%- include('../templates/footer'); -%>
<script src="../../script/datatable.js"></script>
</body>
</html>